<mapper namespace="com.hand.demo.infra.mapper.UserMapper">


    <sql id = "BaseSql">
        tu.creation_date, 
        tu.created_by, 
        tu.last_updated_by, 
        tu.last_update_date, 
        tu.object_version_number
